    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-local": "^1.0.38",
    "@types/sha256": "^0.2.2",
    "bluebird": "^3.7.2",
    "bootstrap-icons": "^1.11.3",
    "concurrently": "^7.6.0",
