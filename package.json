{
  "name": "dexchat",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start": "nodemon build/server.js",
    "dev": "   nodemon --exec tsx server.ts",
    "prebuild": "rm -rf build",
    "build": "npm install ; tsc --outDir build ",
    "postbuild": "cd build ; mkdir -p public/images/users",
    "devbuild": " tsc --outDir build ",
    "bdev": "nodemon --exec tsx server.ts",
    "bfdev": "concurrently 'npm run bdev' 'cd dexchat && npm start' "
  },
